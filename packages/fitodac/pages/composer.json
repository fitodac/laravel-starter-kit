{
	"name": "fitodac/pages",
	"description": "A Laravel package for creating dynamic pages with Filament and Inertia",
	"version": "1.0.0",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "fitodac",
			"email": "fitodac@gmail.com"
		}
	],
	"require": {
		"php": "^8.2",
		"laravel/framework": "^12.0",
		"filament/filament": "^3.0",
		"inertiajs/inertia-laravel": "^2.0",
		"spatie/laravel-permission": "^6.0"
	},
	"autoload": {
		"psr-4": {
			"Fitodac\\Pages\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Fitodac\\Pages\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Fitodac\\Pages\\PagesServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
